

<div class="ui segment">
  {{tale-browser models=model.tales paginateOn=3 cardsPerRow="three" modelView="tale.view" showSearch=true showLink=true showDelete=true
                 addButtonName="Compose and publish a new tale."
                 addButtonTagName="Compose New Tale"
                 addButtonLogo=""
                 onAddNew=(action "gotoPublish")
  }}
</div>


<div class="ui stackable grid">

    <div class="four wide column">
        <i class="huge dashboard icon"></i>
        Compute

      {{wt-flix models=model.images paginateOn=3 cardsPerRow="one" modelView="image.view" showLink=true
                addButtonName=null
                addButtonTagName=null
                addButtonLogo=""
                onAddNew=(action "clickedAddNewResearchEnvironment")
                model_is_image=true
      }}

    </div>
    <div class="twelve wide column">
        <i class="huge computer icon"></i>
        Catalog

    </div>


</div>
